#include <bits/stdc++.h>
 
using namespace std;
 
int main()
{
    int T,n;
    cin>>T;
    for(int i=0;i<T;++i)
    {
        double A(0),G(1),H(0),x;
        cin>>n;
 
        for(int j=0;j<n;++j)
    {
        cin>>x;
        A+=x;
        G=G*x;
        H+=(1/x);
 
 
    }
    A= (A/n);
    G=pow(G,1./n);
    H=(n/H);
    cout<<fixed<<setprecision(9)<<A<<" "<<G<<" "<<H<<endl;
    }
}
 
 
 
 
